@model  KOL_HoangThuyLinh.Models.tblZone

<form id="frmUrl" class="form-horizontal form-label-left" style="padding-top:15px">
    @Html.Hidden("Id")
    @Html.Hidden("IdType")
    <div class="item form-group">
        @Html.Label("Url hình", htmlAttributes: new { @class = "col-form-label col-md-3 col-sm-3 label-align" })
        <div class="col-md-9">
            @Html.Editor("urlImage", new { htmlAttributes = new { @class = "form-control required" } })
            @Html.ValidationMessage("urlImage", "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="item form-group">
        <div class="col-md-4 col-sm-4 offset-md-3">
            <button id="btnCancel" class="btn btn-warning" onclick="$('#frmdlg').modal('hide');return false;"><i class="fa fa-ban"></i>&nbsp;Hủy</button>
            <button id="btnSave" class="btn btn-primary" onclick="SaveUrl(); return false;"><i class="fa fa-floppy-o"></i>&nbsp;Chọn</button>
        </div>
    </div>
</form>


