@using KOL_HoangThuyLinh.Models
@using System.Web.Configuration;
@model  tblZone

@{
    string url = WebConfigurationManager.AppSettings["UrlHost"];
    string strLink = url + myCommon.FriendlyUrl(ViewBag.TypeName) + "-" + ViewBag.Id + "c";
}

<div class="container my-5">
    <div class="row">
        <div class="col-md-8 col-sm-12 col-xs-12">
            <div class="th-single-detailv5" id="dvList">
            </div>
        </div>
        <div class="col-md-4 col-sm-12 col-xs-12">
            @{Html.RenderAction("GetLatesNews", "Home"); }
        </div>
    </div>
</div>

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            LoadData();
        });

        function LoadData() {
            $.ajax({
                url: "@Url.Action("GetList", "TypeNews")",
                type: "POST",
                data: {
                    Id:@Html.Raw(ViewBag.Id)
                },
                success: function (msg) {
                    $("#dvList").html(msg);
                }
            })
        }
    </script>
}